            </li>
        </ul>
    </nav>
    <div class="main-content">
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; 2018 - CustomerDataProcess</p>
            </footer>
        </div>
    </div>

